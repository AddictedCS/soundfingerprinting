<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="linq2db" type="LinqToDB.Configuration.LinqToDBSection, linq2db" requirePermission="false"/>
  </configSections>
  
  
  <connectionStrings>
    <clear/>
    <add name="SQLite"           connectionString="Data Source=TestData.sqlite" providerName="SQLite"/>
    <add name="FingerprintConnectionString" connectionString="Data Source=(local);Initial Catalog=fingerprintstest;Integrated Security=True; Connection Timeout=15;" providerName="System.Data.SqlClient"/>
  </connectionStrings>
  <system.diagnostics>
    <switches>
      <add name="trace" value="0"/>
    </switches>
    <trace autoflush="true" indentsize="5">
      <listeners>
        <clear/>
        <add name="DemoListener" type="System.Diagnostics.TextWriterTraceListener" initializeData="output.txt"/>
        <remove name="Default"/>
      </listeners>
    </trace>
  </system.diagnostics>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/></startup></configuration>
